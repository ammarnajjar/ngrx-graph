{
  "name": "ngrx-graph",
  "version": "0.0.10",
  "description": "Generate NgRx actions graph",
  "author": "Ammar Najjar",
  "bin": {
    "ngrx-graph": "./bin/run"
  },
  "homepage": "https://github.com/ammarnajjar/ngrx-graph",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "ammarnajjar/ngrx-graph",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2.11.8",
    "@oclif/plugin-help": "^5.2.19",
    "fast-glob": "^3.3.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@oclif/test": "^2.5.3",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.17.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "chai": "^4.3.8",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "oclif": "^3.11.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "oclif": {
    "bin": "ngrx-graph",
    "dirname": "ngrx-graph",
    "commands": "./dist/commands",
    "default": "graph",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "graph": {
        "description": "Generate NgRx actions graph"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "npm run build && rm tsconfig.tsbuildinfo && oclif manifest && oclif readme",
    "test": "mocha --watch  --watch-files . \"test/**/*.test.ts\"",
    "test:ci": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/ammarnajjar/ngrx-graph/issues",
  "keywords": [
    "oclif, ngrx, graph, action, effect, angular, component, reducer, store, state, dot"
  ],
  "types": "dist/index.d.ts"
}
